{% extends 'base.html.twig' %}

{% block title %}Nous contacter
{% endblock %}

{% block body %}
	<main id="contact">
		<section>
			<h1>Nous contacter</h1>

			{{ form_start (form)}}
			<div>
				{{form_row(form.name)}}
			</div>
			<div>
				{{form_row(form.email)}}

			</div>

			{{ form_end (form)}}
		</section>
	</main>
{% endblock %}
